  "keywords": ["openId", "wsse", "authentication"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxWsseBundle/contributors"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.8"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Sfynx\\WsseBundle": "" }
